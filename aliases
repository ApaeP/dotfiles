function lolz() {
  RANDOM=$(date +%s)
  export GITHUB_USERNAME=`gh api user | jq -r '.login'`
  command="(sleep $(( $RANDOM % 10 )); osascript -e \"set Volume 10\"; say -v Thomas \"Je suis hilarant\";) &"
  echo $command > ~/code/$GITHUB_USERNAME/dotfiles/zshrc
}
