# Git
# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

alias hb="gh browse"

function gitall() {
  git add .
  if [ "$1" != "" ]; then
    git commit -m "$1"
  else
    git commit -m update
  fi
  git push origin HEAD
}

# Cherry-pick a range of commits (first hash = commit before range, second = last to include)
function cplist() {
  git cherry-pick $1
  for commit in $(git log $1..$2 --reverse --pretty=%H); do
    git cherry-pick $commit
  done
}

# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
# PostgreSQL
# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

function psql_service() {
  case $1 in
    start)   brew services start postgresql@17 ;;
    stop)    brew services stop postgresql@17 ;;
    restart) brew services restart postgresql@17 ;;
    *)       echo "Usage: psql_service {start|stop|restart}" ;;
  esac
}

function fixpsql() {
  brew services stop postgresql@17 && \
  rm -f /opt/homebrew/var/postgresql@17/postmaster.pid && \
  brew services start postgresql@17 && \
  brew services list
}

function psql_disconnect_all() {
  local db_name=$1
  local terminal_cols=$(tput cols)
  local query

  if [ -n "$db_name" ]; then
    query="SELECT pg_terminate_backend(pid) FROM pg_stat_activity WHERE datname='$db_name' AND pid <> pg_backend_pid() AND wait_event IS NULL;"
    echo -e "\n\e[38;5;36m\e[1m$(printf '%*s' $(((${#db_name} + 45 + terminal_cols) / 2)) "Disconnecting all connections to the database: $db_name")\e[0m\n"
  else
    query="SELECT pg_terminate_backend(pid) FROM pg_stat_activity WHERE pid <> pg_backend_pid() AND wait_event IS NULL;"
    echo -e "\n\e[38;5;36m\e[1m$(printf '%*s' $(((38 + terminal_cols) / 2)) "Disconnecting all PostgreSQL connections")\e[0m\n"
  fi

  psql -c "$query" postgres
  echo -e "\n\e[38;5;82m\e[1m$(printf '%*s' $(((24 + terminal_cols) / 2)) "Connections terminated")\e[0m\n"
}

function psql_connections() {
  local db_name=$1
  local terminal_cols=$(tput cols)
  local query

  if [ -n "$db_name" ]; then
    echo -e "\n\e[38;5;36m\e[1m$(printf '%*s' $(((${#db_name} + 43 + terminal_cols) / 2)) "Active connections to the database: $db_name")\e[0m\n"
    query="SELECT pid, datname, usename, application_name, client_addr, state, query_start, NOW() - query_start AS duration, state_change, wait_event_type, wait_event, query FROM pg_stat_activity WHERE datname='$db_name' ORDER BY query_start DESC;"
  else
    echo -e "\n\e[38;5;36m\e[1m$(printf '%*s' $(((34 + terminal_cols) / 2)) "All active PostgreSQL connections")\e[0m\n"
    query="SELECT pid, datname, usename, application_name, client_addr, state, query_start, NOW() - query_start AS duration, state_change, wait_event_type, wait_event, query FROM pg_stat_activity ORDER BY query_start DESC;"
  fi

  psql -c "$query" postgres
}

# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
# Docker
# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

dc() {
  COMPOSE_BAKE=true
  case $1 in
    start)
      shift
      command brew services stop postgresql@17
      ;;
    stop)
      shift
      command brew services start postgresql@17
      ;;
    up)
      shift
      if [ $# -eq 0 ]; then
        command docker-compose --profile sline-api up -d && docker attach sline-api
      else
        command docker-compose up "$@" -d && docker attach "$@"
      fi
      ;;
    build)
      shift
      if [ $# -eq 0 ]; then
        command docker-compose --profile sline-api build
      else
        command docker-compose build "$@"
      fi
      ;;
    down)
      shift
      command docker-compose --profile sline-api down
      ;;
    c)
      shift
      command docker exec -it -t "$@" /bin/bash
      ;;
    a)
      shift
      command docker attach "$@"
      ;;
    logs)
      shift
      command docker-compose logs -f "$1"
      ;;
    clean)
      shift
      command docker rm -f $(docker ps -aq) 2>/dev/null || true
      command docker rmi -f $(docker images -q) 2>/dev/null || true
      command docker volume rm $(docker volume ls -q) 2>/dev/null || true
      command docker network rm $(docker network ls -q -f "type=custom") 2>/dev/null || true
      command docker builder prune --force --all
      command docker buildx rm $(docker buildx ls --format '{{.Name}}' | grep -v default)
      command docker system prune -a --volumes --force
      ;;
    *)
      command docker-compose "$@"
      ;;
  esac
}

